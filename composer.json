{
    "name": "limingxinleo/i-cache",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "Illuminate Cache for Hyperf",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Illuminate\\Cache\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "hyperf/di": "^3.0",
        "hyperf/utils": "^3.0",
        "nesbot/carbon": "^3.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "hyperf/config": "^3.0",
        "hyperf/redis": "^3.0",
        "limingxinleo/happy-join-hyperf": "^1.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": ">=7.0",
        "swoole/ide-helper": "dev-master"
    },
    "replace": {
        "illuminate/cache": "*"
    },
    "suggests": {
        "hyperf/redis": "Required to use redis driver. (^2.0)"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 1 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "hyperf": {
            "config": "Illuminate\\Cache\\ConfigProvider"
        }
    }
}
